<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>

<BorderPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proj.selekcjanatur.AppController">
    <top>
        <Button fx:id="pauseButton" alignment="CENTER" onAction="#togglePause" text="Pauza">
            <BorderPane.margin>
                <Insets left="2.0" />
            </BorderPane.margin>
        </Button>
    </top>
    <center>
        <GridPane fx:id="grid" />
    </center>
</BorderPane>
